{
  "name": "json-tree-view",
  "displayName": "JSON Tree View",
  "description": "Generate tree view from json file",
  "version": "0.0.1",
  "publisher": "Chauncey",
  "engines": {
    "vscode": "^1.20.0"
  },
  "activationEvents": [
    "onView:jsonTreeView",
    "onLanguage:json",
    "onLanguage:jsonc"
  ],
  "main": "./out/extension",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "jsonTreeView",
          "name": "Json Tree View",
          "when": "jsonTreeViewEnabled"
        }
      ]
    },
    "commands": [
      {
        "command": "jsonTreeView.refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "jsonTreeView.refreshNode",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "jsonTreeView.renameNode",
        "title": "Rename"
      },
      {
        "command": "jsonTreeView.revealNode",
        "title": "Reveal"
      },
      {
        "command": "jsonTreeView.revealNodeWithKey",
        "title": "RevealWithKey"
      },
      {
        "command": "extension.openJsonSelection",
        "title": "Open JSON Selection"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "jsonTreeView.refresh",
          "when": "view == jsonTreeView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "jsonTreeView.revealNode",
          "when": "view == jsonTreeView"
        },
        {
          "command": "jsonTreeView.revealNodeWithKey",
          "when": "view == jsonTreeView"
        }
      ]
    },
    "configuration": [
      {
        "title": "JSON Tree View",
        "properties": {
          "jsonTreeView.autorefresh": {
            "type": "boolean",
            "default": true
          },
          "jsonTreeView.customizedViewActivated": {
            "type": "boolean",
            "default": true
          },
          "jsonTreeView.customizedViewMapping": {
            "type": "object"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "jsonc-parser": "^0.4.2"
  }
}
